<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <description>Empty web.xml file for Web Application</description>
  <servlet>
    <servlet-name>addSolution</servlet-name>
    <servlet-class>mypackage1.addSolution</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>searchSolution</servlet-name>
    <servlet-class>mypackage1.searchServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ViewSolution</servlet-name>
    <servlet-class>mypackage1.ViewSolution</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>editSolution</servlet-name>
    <servlet-class>mypackage1.editSolution</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>updateSolution</servlet-name>
    <servlet-class>mypackage1.updateSolution</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>mypackage1.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddUser</servlet-name>
    <servlet-class>mypackage1.AddUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>mytest</servlet-name>
    <servlet-class>mypackage1.mytest</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getBlob</servlet-name>
    <servlet-class>mypackage1.getBlob</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addSolution</servlet-name>
    <url-pattern>/addSolution</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>searchSolution</servlet-name>
    <url-pattern>/searchSolution</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ViewSolution</servlet-name>
    <url-pattern>/viewSolution</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>editSolution</servlet-name>
    <url-pattern>/editSolution</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>updateSolution</servlet-name>
    <url-pattern>/updateSolution</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddUser</servlet-name>
    <url-pattern>/adduser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>mytest</servlet-name>
    <url-pattern>/mytest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getBlob</servlet-name>
    <url-pattern>/getBlob</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>35</session-timeout>
  </session-config>
  <mime-mapping>
    <extension>html</extension>
    <mime-type>text/html</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>txt</extension>
    <mime-type>text/plain</mime-type>
  </mime-mapping>
</web-app>
